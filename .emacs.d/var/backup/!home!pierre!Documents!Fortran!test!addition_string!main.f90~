program main
  use m_kind

  real(sp),parameter :: simple=1.0_sp
  real(dp),parameter :: double=1.0_dp
  
  real(mp),parameter :: my_precision=1.0_mp

  print*,'test simple precision :',simple
  print*,'test double precision :',double
  print*,'test personalize precision :',my_precision

  print*,'test on the fly simple :',1.0_sp
  print*,'test on the fly double :',1.0_dp

  print*,'test my precision :',my_precision

  print*,'test, my precision est de precision :',kind(my_precision)
  
end program main
